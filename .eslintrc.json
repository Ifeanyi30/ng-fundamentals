{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-empty-function":"off",
        "@typescript-eslint/ban-types": ["off"
            // ,{
            //     "types": {
            //         "String": false,
            //         "Boolean": false,
            //         "Number": false,
            //         "Symbol": false,
            //         "{}": false,
            //         "Object": false,
            //         "object": false,
            //         "Function": false
            //     },
            //     "extendDefaults": true
            // }
        ],
        "@typescript-eslint/no-non-null-assertion": [
            "off"
        ],
        "@typescript-eslint/no-explicit-any": [
            "off"
        ]
    }
}
